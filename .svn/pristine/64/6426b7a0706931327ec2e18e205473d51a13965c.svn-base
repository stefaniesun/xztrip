		<mapping class="xyz.model.member.Customer"></mapping>
		<mapping class="xyz.model.member.CustomerUserTag"></mapping>
		
		<!-- 支付宝付款流水 -->
		<mapping class="xyz.model.core.Bill"></mapping>
		<mapping class="xyz.model.member.WeixinUserInfo"></mapping>
		<mapping class="xyz.model.core.SeekHelp"></mapping>
		
		<mapping class="xyz.model.main.UserTag"></mapping>
		<!-- 微信支付账单流水 -->
